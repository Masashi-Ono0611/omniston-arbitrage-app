{
  "name": "@ston-fi/omniston-sdk-react",
  "version": "0.7.7",
  "description": "React SDK for the STON.fi Omniston protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ston-fi/omniston-sdk.git",
    "directory": "packages/omniston-sdk-react"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "tsdown --sourcemap --watch ./src",
    "build": "tsdown",
    "lint": "tsc --noEmit --pretty",
    "test": "vitest",
    "prepublishOnly": "turbo run build --filter=@ston-fi/omniston-sdk && pnpm run lint && pnpm run test --run && pnpm run build && pnpm attw --pack . && pnpm publint"
  },
  "dependencies": {
    "@ston-fi/omniston-sdk": "workspace:*",
    "@tanstack/react-query": "5.49.2",
    "rxjs": "7.8.1"
  },
  "peerDependencies": {
    "react": "^18 || ^19"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "catalog:",
    "@types/react": "^19",
    "publint": "catalog:",
    "tsdown": "catalog:",
    "vitest": "catalog:"
  }
}
